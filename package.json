    "babel-preset-es2016": "^7.0.0-alpha.17",
    "babel-preset-es2017": "^7.0.0-alpha.17",
    "babel-preset-react": "^7.0.0-alpha.17",
    "babel-preset-stage-0": "^7.0.0-alpha.17",
    "babel-runtime": "^7.0.0-alpha.17",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "css-loader": "0.28.4",
    "dotenv": "4.0.0",
    "es2015": "0.0.0",
    "eslint": "3.19.0",
    "eslint-config-react-app": "^1.0.5",
    "eslint-loader": "1.7.1",
    "eslint-plugin-flowtype": "2.34.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-react": "7.1.0",
    "extract-text-webpack-plugin": "2.1.2",
    "file-loader": "0.11.2",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "jest": "20.0.4",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "node-sass": "^4.5.3",
    "npm": "^5.3.0",
    "postcss-flexbugs-fixes": "3.0.0",
    "postcss-loader": "2.0.6",
    "resolve-url-loader": "^2.1.0",
    "sass-loader": "^6.0.6",
